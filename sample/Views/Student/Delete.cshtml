@model StudentData.Student

<h2>Delete</h2>
@using (Html.BeginForm("delete", "Student", FormMethod.Post, new { id = "popupForm" }))
{
    @Html.HiddenFor(s => s.EnrollmentModelsID)
    <div class="form-group">
        <label>FirstMid Name</label>
        <p>@Model.FirstMidName</p>
    </div>
    <div class="form-group">
        <label>Last Name</label>
        <p>@Model.LastName</p>
    </div>
    <div class="form-group">
        <label>Title</label>
        <p>@Model.Title</p>
    </div>

    <div>
        <input type="submit" value="Delete"/>
    </div>

    <div class="form-group">
        <p id="error"></p>
    </div>
}
